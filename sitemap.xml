---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% for page in site.pages %}
  {% unless page.url contains '404' or page.sitemap == false %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ page.url | relative_url }}</loc>
    <lastmod>{{ page.date | date_to_xmlschema | default: site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% endunless %}
  {% endfor %}
</urlset>
